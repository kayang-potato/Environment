{
    // "remote.SSH.remotePlatform": {
    //     "linux": "linux"
    // },

    "workbench.colorCustomizations":{
        "editor.selectionBackground":"#79a582b7",
        "editor.selectionHighlightBackground": "#b0a0ce8c"
    },
    "python.pythonPath": "/opt/homebrew/anaconda3/bin/python",
    //  以下python配置，请先安装python 插件
    // yapf 配置 需pip install yapf  ---------------------
    "python.formatting.provider": "yapf",
    // pylint 配置 需 pip install pylint -----------------------
    "python.linting.pylintEnabled": true, //
    // 关闭最小化检测（全检查）
    "python.linting.pylintUseMinimalCheckers": false,

    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "window.zoomLevel": 1,
    // C++ 配置  -----------------------
    // clang-format 配置 ,需安装 clang-format 插件 ----------------------------
    "clang-format.executable": "/opt/homebrew/bin/clang-format", // clang-format 路径
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format" //默认的格式化引擎
    },
    // clang-tidy 配置 需安装 clang-tidy 插件 --------------------------
    "clang-tidy.executable": "/opt/homebrew/opt/llvm/bin/clang-tidy", // clang-tidy 路径
    "clang-tidy.buildPath": "${workspaceFolder}/compile_commands.json", // 编译命令文件，clang-tidy会通过此文件中的编译命令来寻找头文件等配置
    "fileheader.customMade": {
      "Author": "Yang Kaihang, yang_kaihang@dahuatech.com",
      "Date": "Do not edit",
      "LastEditors": "Yang Kaihang",
      "LastEditTime": "Do not edit",
      "FilePath": "Do not edit",
      "Description": "file content",
      "custom_string_obkoro1_copyright": "Copyright (c) ${now_year} by Yang Kaihang, All Rights Reserved. "
    },
    // 函数注释
    "fileheader.cursorMode": {
      "description": "", // 函数注释生成之后，光标移动到这里
      "param": "", // param 开启函数参数自动提取 需要将光标放在函数行或者函数上方的空白行
      "return": ""
    },
    "[jsonc]": {

      "editor.quickSuggestions": {
        "strings": true
      },
      "editor.suggest.insertMode": "replace"
    },
    "editor.codeActionsOnSave": null,
    "terminal.integrated.shell.linux": "/bin/zsh",
    // "fileheader.Author": "Yang Kaihang",
    // "fileheader.LastModifiedBy": "Yang Kaihang",
    // "fileheader.comments": "file content",
    "terminal.integrated.automationShell.windows": "",
    "files.trimTrailingWhitespace": true,
    "fileheader.configObj": {

      "createFileTime": true,
      "language": {
        "languagetest": {
          "head": "/$$",
          "middle": " $ @",
          "end": " $/",
          "functionSymbol": {
            "head": "/** ",
            "middle": " * @",
            "end": " */"
          },
          "functionParams": "js"
        }
      },
      "autoAdd": false,
      "autoAddLine": 100,
      "autoAlready": true,
      "annotationStr": {
        "head": "/*",
        "middle": " * @",
        "end": " */",
        "use": false
      },
      "headInsertLine": {
        "php": 2,
        "sh": 2
      },
      "beforeAnnotation": {
        "文件后缀": "该文件后缀的头部注释之前添加某些内容"
      },
      "afterAnnotation": {
        "文件后缀": "该文件后缀的头部注释之后添加某些内容"
      },
      "specialOptions": {
        "特殊字段": "自定义比如LastEditTime/LastEditors"
      },
      "switch": {
        "newlineAddAnnotation": true
      },
      "supportAutoLanguage": [],
      "prohibitAutoAdd": [
        "json"
      ],
      "folderBlacklist": [
        "node_modules",
        "文件夹禁止自动添加头部注释"
      ],
      "prohibitItemAutoAdd": [
        "项目的全称, 整个项目禁止自动添加头部注释, 可以使用快捷键添加"
      ],
      "moveCursor": true,
      "dateFormat": "YYYY-MM-DD HH:mm:ss",
      "atSymbol": [
        "@",
        "@"
      ],
      "atSymbolObj": {
        "文件后缀": [
          "头部注释@符号",
          "函数注释@符号"
        ]
      },
      "colon": [
        ": ",
        ": "
      ],
      "colonObj": {
        "文件后缀": [
          "头部注释冒号",
          "函数注释冒号"
        ]
      },
      "filePathColon": "路径分隔符替换",
      "showErrorMessage": false,
      "writeLog": false,
      "wideSame": false,
      "wideNum": 13,
      "functionWideNum": 0,
      "CheckFileChange": false,
      "createHeader": false,
      "useWorker": false,
      "designAddHead": false,
      "headDesignName": "random",
      "headDesign": false,
      "cursorModeInternalAll": true,
      "openFunctionParamsCheck": true,
      "functionParamsShape": [
        "[",
        "]"
      ],
      "functionBlankSpaceAll": {},
    //   "functionTypeSymbol": "*",
      "typeParamOrder": "type param",
      "customHasHeadEnd": {},
      "throttleTime": 60000,
    //   "functionParamAddStr": "",
      "NoMatchParams": "no show param",
      "cursorModeInternal": true,
      "functionBlankSpace": 4, // 默认不缩进
      "functionTypeSymbol": "match param no type",
      "functionParamAddStr": ": "
    },
    "settingsSync.ignoredExtensions": [],
    "workbench.startupEditor": "none",
    "C_Cpp.intelliSenseEngine": "Disabled",
    "extensions.ignoreRecommendations": true,
    "python.dataScience.sendSelectionToInteractiveWindow": false,
    "jupyter.askForKernelRestart": false,
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "update.mode": "none",
    // "http.proxyAuthorization": null,
    // "http.proxy": "http://127.0.0.1:8080"

}
